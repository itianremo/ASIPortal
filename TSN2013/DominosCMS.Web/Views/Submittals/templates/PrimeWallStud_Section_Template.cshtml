@model DominosCMS.Models.SubmittalItem

@{
    Layout = "~/Views/Shared/_members.cshtml";
    decimal webDepth = Convert.ToDecimal(Model.Section.WebDepth);
    var notes = ViewBag.Notes as IList<DominosCMS.Models.Note>;
    var bcode = ViewBag.BuildingCode;
    var vals = Model.Section.Values.Where(v => v.BuildingCode == bcode).OrderBy(v => v.Property.ViewOrder).ToList();
    
}

@section right {
    <img src="/_static/images/PrimeWallStud2.jpg"  width="299" height="152"/><br />

    @if (webDepth > 250) {    
        <img src="/_static/images/PrimeWallStud350.jpg"  /><br />
    <p>
        Standard punchouts have a length of 2.5" and width of 1.5" located along the centerline of the web 24" o.c.
    </p>

    <p>
        Typical western punchout spacing 24" from lead end, then 24" o.c.
    </p>
    <p>
        Typical eastern punchout spacing 12" from lead end, then 24" o.c.
    </p>
    } else {
        <img src="/_static/images/PrimeWallStud162-250.jpg"  /><br />
  <p>Standard punchouts have a length of 1.75" and width of 0.77" located along the centerline of the web 24" o.c.    </p>

    <p>Typical western punchout spacing 24" from lead end, then 24" o.c.
    </p>
    <p>Typical eastern punchout spacing 12" from lead end, then 24" o.c.
    </p>
    }

    
}
