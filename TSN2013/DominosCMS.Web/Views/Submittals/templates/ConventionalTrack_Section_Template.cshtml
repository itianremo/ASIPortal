@model DominosCMS.Models.SubmittalItem

@{
    Layout = "~/Views/Shared/_members.cshtml";
    var notes = ViewBag.Notes as IList<DominosCMS.Models.Note>;
    var bcode = ViewBag.BuildingCode;
    var vals = Model.Section.Values.Where(v => v.BuildingCode == bcode).OrderBy(v => v.Property.ViewOrder).ToList();
}

@section right {
    <img src="/_static/images/ConventionalTrack.jpg" width="299"  height="143" /><br />

}

            <table style="width:100%">
                <tr>
                    <th colspan="3">Product Profile</th>
                </tr>
                <tr class="alt">
                    <td>Web Depth (D)</td>
                    <td class="val-col">@vals[0].Value </td><td>in</td>
                </tr>
                <tr>
                    <td>Leg Length (B)</td>
                    <td class="val-col">@vals[1].Value </td><td>in</td>
                </tr>
                <tr class="alt">
                    <td>Gauge</td>
                    <td class="val-col">@vals[2].Value </td><td>ga</td>
                </tr>
                <tr>
                    <td>Design Thickness (t)</td>
                    <td class="val-col">@vals[3].Value </td><td>in</td>
                </tr>
                <tr class="alt">
                    <td>Minimum Steel Thickness (t<sub>min</sub>)</td>
                    <td class="val-col">@vals[4].Value </td><td>in</td>
                </tr>
                <tr>
                    <td>Inside Bend Radius (R)</td>
                    <td class="val-col">@vals[5].Value </td><td>in</td>
                </tr>

                <tr>
                    <th colspan="3">Material Composition</th>
                </tr>
                <tr class="alt">
                    <td  >ASTM Standard:</td><td colspan="2"> @vals[6].Value</td>
                </tr>
                <tr>
                    <td>Yield Strength</td>
                    <td class="val-col">@vals[7].Value </td><td>ksi (MPa)</td>
                </tr>
                <tr class="alt">
                    <td>Tensile Strength</td>
                    <td class="val-col">@vals[8].Value </td><td>ksi (MPa)</td>
                </tr>
                <tr>
                    <td>Coating Weight</td>
                    <td class="val-col">@vals[9].Value </td><td></td>
                </tr>
                <tr>
                    <th colspan="3">Gross Properties</th>
                </tr>
                <tr class="alt">
                    <td>Area</td>
                    <td class="val-col">@vals[10].Value </td><td>in<sup>2</sup></td>
                </tr>
                <tr>
                    <td>Unit Weight </td>
                    <td class="val-col">@vals[11].Value </td><td>(lbs/ft)</td>
                </tr>
                <tr class="alt">
                    <td>Moment of Inertia (I<sub>x</sub>)</td>
                    <td class="val-col">@vals[12].Value </td><td>in<sup>4</sup></td>
                </tr>
                <tr>
                    <td>Section Modulus (S<sub>x</sub>)</td>
                    <td class="val-col">@vals[13].Value </td><td>in<sup>3</sup></td>
                </tr>
                <tr class="alt">
                    <td>Radius of Gyration (R<sub>x</sub>)</td>
                    <td class="val-col">@vals[14].Value </td><td>in</td>
                </tr>
                <tr>
                    <td>Moment of Inertia (I<sub>y</sub>)</td>
                    <td class="val-col">@vals[15].Value </td><td>in<sup>4</sup></td>
                </tr>
                <tr class="alt">
                    <td>Radius of Gyration (R<sub>y</sub>)</td>
                    <td class="val-col">@vals[16].Value </td><td>in</td>
                </tr>
                <tr>
                    <th colspan="3">Torsional Properties</th>
                </tr>
                <tr class="alt">
                    <td>St. Venant Torsion Constant (Jx1000)</td>
                    <td class="val-col">@vals[17].Value </td><td>in<sup>4</sup></td>
                </tr>
                <tr>
                    <td>Warping Constant (C<sub>w</sub>)</td>
                    <td class="val-col">@vals[18].Value </td><td>in<sup>6</sup></td>
                </tr>
                <tr class="alt">
                    <td>Distance from Shear Center to Neutral Axis (X<sub>o</sub>)</td>
                    <td class="val-col">@vals[19].Value </td><td>in</td>
                </tr>
                <tr>
                    <td>Distance Between Shear Center and Web Centerline (m)</td>
                    <td class="val-col">@vals[20].Value </td><td>in</td>
                </tr>
                <tr class="alt">
                    <td>Polar Radius of Gyration (R<sub>o</sub>)</td>
                    <td class="val-col">@vals[21].Value </td><td>in</td>
                </tr>
                <tr>
                    <td>Torsional Flexural Constant (β)</td>
                    <td class="val-col">@vals[22].Value</td><td></td>
                </tr>
                <tr>
                    <th colspan="3">Effective Properties</th>
                </tr>
                <tr>
                    <td>Moment of Inertia for Deflection (I<sub>x</sub>)</td>
                    <td class="val-col">@vals[23].Value </td><td>in<sup>4</sup></td>
                </tr>
                <tr class="alt">
                    <td>Section Modulus (S<sub>x</sub>)</td>
                    <td class="val-col">@vals[24].Value </td><td>in<sup>3</sup></td>
                </tr>
                <tr>
                    <td>Allowable Bending Moment (M<sub>a</sub>)</td>
                    <td class="val-col">@vals[25].Value </td><td>kips-in</td>
                </tr>
                <tr class="alt">
                    <td>Allowable Gross Shear (V<sub>ag</sub>)</td>
                    <td class="val-col">@vals[26].Value </td><td>lb</td>
                </tr>

            </table>


@section comments {
<ul  class="no-bullet" style="text-align: left">
         @if (notes != null) {
   
        foreach (var note in notes)
        {
            <li class="note">
                <span><sup>@note.Number</sup></span> @note.Text
            </li>
        }
    
    }
    </ul>
    <ul style="margin-top: 0;">

        <li>Section properties and capacities are calculated in accordance with AISI‐NASPEC 2007</li>
        <li>Tabulated gross properties are based on the full, unreduced cross section of the track.</li>
        <li>Effective section properties incorporate the strength increase from cold work of forming as applicable per AISI‐NASPEC, Sec. A7.2.</li>
	<li>Net effective section properties are calculated at a cross section through the punchout.</li>
	<li>Structural framing is produced to meet or exceed ASTM C955, A653, and A1003.</li>
        <li>Non-Structural framing is produced to meet or exceed ASTM C645, A653, and A1003.</li>
        <li>For deflection calculations, use the effective moment of inertia (I<sub>x</sub>). This is calculated at a stress which results in an a section modulus such that the stress times the section modulus at that stress is
            equal to the allowable moment.</li>
    </ul>
    
}